{
  "name": "@lanetix/odata-parser",
  "version": "6.1.2",
  "description": "OData query string parser",
  "main": "lib/index.js",
  "scripts": {
    "flow": "flow check --show-all-errors",
    "build": "./add.flow.js",
    "test": "npm run-script prepublish && npm run-script flow && mocha -R spec --opts test/mocha.opts",
    "prepublish": "rm -rf lib && mkdir lib && pegjs -o lib/index.js src/odata.pegjs && npm run-script build",
    "repl": "npm run prepublish && node repl.js"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "flow-bin": "^0.50.0",
    "mocha": "2.1.0",
    "pegjs": "0.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auth0/node-odata-parser"
  },
  "keywords": [
    "odata",
    "parser"
  ],
  "author": "auth0",
  "license": "MIT",
  "dependencies": {
    "flow-runtime": "^0.14.0"
  }
}
